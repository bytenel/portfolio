@font-face {
	font-family: "Flick Bold Hollow";
	src: url('<%= asset_path('fonts/FLICKHOL.ttf') %>') format('truetype');
}

.image-frame{
	position: relative;
    width: 200px;
	height: 200px;
	border-radius: 150px;
	-webkit-border-radius: 150px;
	-moz-border-radius: 150px;	
	background: url(<%= image_path('me.jpg') %>) no-repeat center center;
	border: 15px solid #80bd01;
	top: 0;
	left:0;
}

.image-container{
	position: absolute;
	bottom: 300px;
	right: 200px;
	bottom: 0;
	right: 0;
}

body{
	font-family: "Flick Bold Hollow";
	font-size: 50px;
	color: #80bd01;
	background: darken(#ccc, 40%);
}

.small-icon{
	height: 75px;
	width: 75px;
	display: none;
}

.inner-image-github-frame{
	position: absolute;
	top: 30px;
	right: 80px;
}

.inner-image-linkedin-frame{
	position: absolute;
	bottom: 30px;
	right: 80px;
}