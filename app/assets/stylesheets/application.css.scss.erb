@import "bootstrap";

#main_content{
	height: 800px;
}


#contactForm{
	width: 50%;
	margin-left:auto;
	margin-right:auto;	
}

.my-image{
	max-width: 1200px;
	max-height: 700px;
	
  /*  border: 2px solid black;
            border-radius: 30px;
            -moz-border-radius: 30px;
            -khtml-border-radius: 30px;
            -webkit-border-radius: 30px; */
}

.tile-wrapper{
	position: absolute;
	height: 800px;
	width: 1700px;
	min-height: 100%;
}

.center-frame{
	position: relative;
    height: 40%;
	width: 40%;
    margin:auto;
    vertical-align: middle;
    background: url(image_path('me.jpg')) no-repeat center center ;
    background-size: 700px 440px;
    top: 25%;
}

.left-frame{
	position: absolute;
	left: 0;
	bottom: 0;
    float: left;
    width: 30%;
    height: 35%;
    background-color: red;
}

.left-frame:hover{
	background-color: lighten(red, 15%);
}

.bottom-frame{
    float: left;
    width: 40%;
    height: 35%;
    position: absolute;
    left: 30%;
    bottom: 0;
    background-color: green;
}

.bottom-frame:hover{
	background-color: lighten(green, 5%);
}

.right-frame{
	position: absolute;
	bottom: 0;
	right: 0;
    width: 30%;
    height: 35%;
    background-color: blue;
}

.right-frame:hover{
	background-color: lighten(blue, 15%);
}

.top-frame {
    width: 40%;
    height: 25%;
    background-color: #DDDDDD;
    float: left;
}

.top-frame:hover{
	background-color: lighten(#ddd, 5%);
}

.left-top-frame{
    width: 30%;
    height: 25%;
    background-color: yellow;
    float: left;
}

.left-top-frame:hover{
	background-color: lighten(yellow, 15%);
}

.right-top-frame{
    float: right;
    width: 30%;
    height: 25%;
    background-color: pink;
}

.right-top-frame:hover{
	background-color: lighten(pink, 5%);
}

.left-center-frame{
    width: 30%;
    height: 40%;
    background-color: black;
    float: left;
}

.left-center-frame:hover{
	background-color: lighten(black, 5%);
}

.right-center-frame{
	position: relative;
	bottom: 15%;
    float: right;
    width: 30%;
    height: 40%;
    background-color: orange;
}

.right-center-frame:hover{
	background-color: lighten(orange,5%);
}