@font-face {
	font-family: "Flick Bold Hollow";
	src: url('<%= asset_path('fonts/FLICKHOL.ttf') %>') format('truetype');
}

.image-frame{
    width: 300px;
	height: 300px;
	border-radius: 150px;
	-webkit-border-radius: 150px;
	-moz-border-radius: 150px;
	background: url(<%= image_path('me.jpg') %>) no-repeat center center;
	border: 15px solid #80bd01;
	position: absolute;
	bottom: 0;
	right: 0;
}

body{
	font-family: "Flick Bold Hollow";
	font-size: 50px;
	color: #80bd01;
	background: darken(#ccc, 40%);
    -webkit-animation-delay: 0.1s;
    -webkit-animation-name: fontfix;
    -webkit-animation-duration: 0.1s;
    -webkit-animation-iteration-count: 1;
    -webkit-animation-timing-function: linear;
}

@-webkit-keyframes fontfix {
    from { opacity: 1; }
    to   { opacity: 1; }
}