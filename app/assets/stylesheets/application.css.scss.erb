@import "bootstrap";

#main_content{
	height: 1000px;
}


#contactForm{
	width: 50%;
	margin-left:auto;
	margin-right:auto;	
}

.my-image{
	max-width: 1200px;
	max-height: 700px;
	height: 445px;
	width: 600px;
    display:block;
    margin:auto;
    vertical-align: middle;
  /*  border: 2px solid black;
            border-radius: 30px;
            -moz-border-radius: 30px;
            -khtml-border-radius: 30px;
            -webkit-border-radius: 30px; */
}

.tile-wrapper{
	height: 100%;
	width: 100%;
	min-height: 100%;
}

.helper{
    display: inline-block;
    height: 100%;
    vertical-align: middle;
}

.center-frame{
    height: 200px;      /* equals max image height */
    width: 100%;
    text-align: center; 
    margin: 0;
}

.left-frame{
    float: left;
    width: 34.9%;
    height: 50%;
    background-color: red;
}

.bottom-frame{
	margin-top: 20%;
    float: left;
    width: 30.1009%;
    height: 50%;
    margin: 0;
    background-color: green;
}


.right-frame{
    float: right;
    width: 35%;
    height: 50%;
    background-color: blue;
}

.top-frame {
    width: 40%;
    height: 40%;
    margin: 0;
    background-color: #DDDDDD;
    float: left;
}

.left-top-frame{
    width: 30%;
    height: 40%;
    background-color: yellow;
    float: left;
}

.right-top-frame{
    float: right;
    width: 30%;
    height: 40%;
    background-color: pink;
}